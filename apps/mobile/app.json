{
  "expo": {
    "name": "TrailDiary",
    "slug": "traildiary",
    "version": "1.0.0",
    "scheme": "traildiary",
    "userInterfaceStyle": "dark",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.traildiary.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#111111"
      },
      "package": "com.traildiary.mobile"
    },
"plugins": [
      "expo-router",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
